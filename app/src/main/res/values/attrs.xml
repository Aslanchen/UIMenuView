<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="UIMenuView">
        <attr name="uimv_title" format="reference|string" />
        <attr name="uimv_image" format="reference|integer" />
        <attr name="uimv_subTitle" format="reference|string" />
        <attr name="uimv_subImage" format="reference|integer" />
        <attr name="uimv_textColorTitle" format="reference|color" />
        <attr name="uimv_textColorSubTitle" format="reference|color" />
        <attr name="uimv_textSizeTitle" format="reference|dimension" />
        <attr name="uimv_textSizeSubTitle" format="reference|dimension" />
        <attr name="uimv_drawablePaddingTitle" format="reference|dimension" />
        <attr name="uimv_drawablePaddingSubTitle" format="reference|dimension" />
        <attr name="uimv_single" format="reference|boolean" />
        <attr name="uimv_maxLength" format="reference|integer" />
        <attr name="uimv_showSub" format="reference|boolean" />
        <attr name="uimv_hint" format="reference|string" />
        <attr name="uimv_inputType">
            <enum name="normal" value="1" />
            <enum name="numberDecimal" value="2" />
            <enum name="phone" value="3" />
            <enum name="number" value="4" />
            <enum name="textPassword" value="5" />
            <enum name="numberPassword" value="6" />
        </attr>
    </declare-styleable>
</resources>